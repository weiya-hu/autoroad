/* pages/project/project.wxss */
.container{background:#ffffff}
.next{width: 30rpx;}
.next1{width: 36rpx;}
.project{background: -webkit-linear-gradient(left,#f1f3f5,#ffffff);padding: 0 20rpx;box-sizing: border-box;font-size: 30rpx;
  height: 100rpx;border-bottom: 0.5rpx solid #ededed;}
.projectson{padding: 0 20rpx;font-size: 30rpx;box-sizing: border-box;padding-left: 80rpx;width: 100%;height: 0;
  opacity: 0;transition: all 0.2s linear;background: -webkit-linear-gradient(left,#f1f3f5,#dddddd);}
.projectson view{color: #444;display: none}
.showpre+view>view{height: 100rpx;opacity: 1;border-bottom: 0.5rpx solid #e1e1e1;}
.showpre+view>view>view{display: inline-block}
.showpre>view:nth-child(2){transform: rotate(180deg)}
.nodataa{position: absolute;top: 50%; left: 50%;transform: translateX(-50%) translateY(-50%);z-index: 99999;width: 100%;
  height: 100%;background: #f1f3f5;}
.nodataimg{width: 300rpx;position: relative;top: -60rpx;}
.nodatatxt{font-size: 24rpx;color:#37A2DA;text-align: center;margin-top: -40rpx; }