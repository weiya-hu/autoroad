.scroll_box{width: 100%;height: 100rpx;overflow: hidden;padding:0 20rpx;background: #fff;
  white-space: nowrap;background: #fff;border-bottom: 1px solid #008aff;}
.address{width: 50%; font-size: 30rpx;display: inline-block;height:100rpx; line-height: 100rpx;text-align: center;
  border-top: 6rpx solid rgba(0, 0, 0, 0);border-bottom: 6rpx solid rgba(0, 0, 0, 0);box-sizing: border-box;
  background: #ffffff}
.active{color: #32C5E9;border-bottom: 6rpx solid #32C5E9;}
.nav {height: 100rpx;width: 100%;box-sizing: border-box;overflow: hidden;line-height: 100rpx;
    background: #ffffff;font-size: 16px;white-space: nowrap;position: fixed;top: 0;left: 0;
    z-index: 99;border-bottom: 1px solid #32C5E9;}
.tab-box{padding-top: 100rpx;height: 100%;box-sizing: border-box;overflow-y: scroll;}
.tab-content{display: flex;flex-wrap:wrap;justify-content:flex-start;align-content: flex-start;height: auto !important; }
.tab{width: 90%;background: #ffffff;border-top: 1rpx solid #ccc;border-left: 1rpx solid #ccc;border-radius: 8rpx;
  margin: 30rpx auto;box-shadow: 4px 4px 4px #ccc}
.row view{text-align: center;font-size: 30rpx;border-right: 1rpx solid #ccc;border-bottom: 1rpx solid #ccc;flex: 1;height: 80rpx;line-height: 80rpx;background: #37A2DA;color: #ffff;}
.rowbro>view{text-align: center;font-size: 26rpx;border-right: 1rpx solid #ccc;border-bottom: 1rpx solid #ccc;flex: 1;height:100rpx;line-height: 100rpx; }
.rd{line-height: 38rpx;text-align: center;overflow : hidden;text-overflow: ellipsis;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}
.noperson{font-size: 30rpx;text-align: center;padding-top: 30rpx;margin: 0 auto;}
.nodataa{position: absolute;top: 50%; left: 50%;transform: translateX(-50%) translateY(-50%);z-index: 99999;width: 100%;
  height: 100%;background: #f1f3f5;}
.nodataimg{width: 300rpx;position: relative;top: -60rpx;}
.nodatatxt{font-size: 24rpx;color:#37A2DA;text-align: center;margin-top: -40rpx; }
.container {background:#f1f3f5;overflow-x:hidden;overflow-y: scroll;}
swiper-item{overflow: visible;}
